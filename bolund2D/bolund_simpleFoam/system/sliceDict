SLICEWIDTH 0.1;
MINX -156.0;
MAXX 250.0;
MINY -15.00;
MAXY #calc "$MINY+$SLICEWIDTH";
MINZ 0.7501;
MAXZ 70.0;

GRADE
{
    X
    {
        N 300;
        S ( (0.3 0.3 0.33) (0.7 0.7 3.0) );
    }
    Y
    {
        N 1;
        S 1;
    }
    Z
    {
        N 64;
        S 6;
    }
}

INPOINT
{
    X 0.0;
    Y #calc "$MINY+$SLICEWIDTH/2.0";
    Z 15.0;
}

SUBGRADE
{
    X
    {
        N 128;
        S ( (0.3 0.3 0.33) (0.7 0.7 3.0) );
    }
    Y
    {
        N 1;
        S 1;
    }
    Z
    {
        N 64;
