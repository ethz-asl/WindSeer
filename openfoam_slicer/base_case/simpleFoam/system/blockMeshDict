FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1.00;
#include "terrainDict"

vertices
(
    ($MINX $MINY $MINZ)
    ($MAXX $MINY $MINZ)
    ($MAXX $MAXY $MINZ)
    ($MINX $MAXY $MINZ)
    ($MINX $MINY $MAXZ)
    ($MAXX $MINY $MAXZ)
    ($MAXX $MAXY $MAXZ)
    ($MINX $MAXY $MAXZ)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($GRADE.X.N $GRADE.Y.N $GRADE.Z.N)
    simpleGrading
    (
        $GRADE.X.S
        $GRADE.Y.S
        $GRADE.Z.S
    )
);

edges
(
);

boundary
(
    north_face
    {
        type empty;
        inGroups (frontAndBack);
        faces
        (
            (3 7 6 2)
        );
    }
    west_face
    {
        type patch;
        inGroups (inlet);
        faces
        (
            (0 4 7 3)
        );
    }
    east_face
    {
        type patch;
        inGroups (outlet);
        faces
        (
            (2 6 5 1)
        );
    }
    south_face
    {
        type empty;
        inGroups (frontAndBack);
        faces
        (
            (1 5 4 0)
        );
    }
    minZ
    {
        type wall;
        faces
        (
            (0 3 2 1)
        );
    }
    maxZ
    {
        type symmetryPlane;
        faces
        (
            (4 5 6 7)
        );
    }     
);

mergePatchPairs
(
);

