TERRAIN_DICT
{
    STL_FILE $STL_FILE;

    MCONVERT $MCONVERT;

    MINX $MINX;
    MAXX $MAXX;
    MINY $MINY;
    MAXY $MAXY;
    MINZ $MINZ;
    MAXZ $MAXZ;

    GRADE
    {
        X { N $NX; S $GX; }
        Y { N $NY; S $GY; }
        Z { N $NZ; S $GZ; }
    }
}

